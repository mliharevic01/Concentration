<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="nyc.ezbar.ce_concentration.MainActivity"
    >

    <ImageView
        android:id="@+id/ezbarBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ezbox"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:id="@+id/location6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_alignTop="@+id/location1"
        android:layout_toEndOf="@+id/textViewreg" />


    <ImageButton
        android:id="@+id/location7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_alignTop="@+id/location2"
        android:layout_alignStart="@+id/location6" />

    <ImageButton
        android:id="@+id/location8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_alignTop="@+id/location3"
        android:layout_alignStart="@+id/location6" />

    <ImageButton
        android:id="@+id/location9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_alignTop="@+id/location4"
        android:layout_alignStart="@+id/location7" />
    <ImageButton
        android:id="@+id/location10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_alignTop="@+id/location5"
        android:layout_alignStart="@+id/location9" />


    <TextView
        android:text="7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView7"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/location7"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:text="8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textview8"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/location8"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:text="10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView10"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/location10"
        android:layout_alignStart="@+id/location10" />

    <TextView
        android:text="9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textview9"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/location9"
        android:layout_alignStart="@+id/textview8" />

    <TextView
        android:text="6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView6"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/location6"
        android:layout_alignEnd="@+id/textView10"
        android:layout_marginEnd="11dp" />

    <TextView
        android:text="@string/register"
        android:textSize="23sp"
        android:textColor="#000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewreg"
        android:layout_marginRight="10dp"

        android:layout_alignTop="@+id/ezbarBox"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp" />

    <ImageButton
        android:id="@+id/location1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_below="@+id/textViewreg"
        android:layout_toStartOf="@+id/textViewreg"
        android:layout_marginEnd="10dp" />

    <TextView
        android:text="1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView1"
        android:textColor="#ffffff"
        android:layout_marginEnd="15dp"
        android:layout_above="@+id/location2"
        android:layout_alignEnd="@+id/location1" />

    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/QuantityInput"
        android:textColor="#000000"
        android:textSize="20sp"
        android:layout_alignBaseline="@+id/QuanitiyLabel"
        android:layout_alignBottom="@+id/QuanitiyLabel"
        android:layout_toEndOf="@+id/QuanitiyLabel" />


    <ImageButton
        android:id="@+id/location2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/location1"
        android:layout_alignStart="@+id/location1" />

    <TextView
        android:text="2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView2"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/location2"
        android:layout_alignStart="@+id/textView1" />

    <ImageButton
        android:id="@+id/location3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"

        android:layout_marginTop="10dp"
        android:layout_below="@+id/location2"
        android:layout_alignStart="@+id/location2" />

    <ImageButton
        android:id="@+id/location4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_below="@+id/location3"
        android:layout_alignStart="@+id/location3"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:id="@+id/location5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/empty"
        android:background="@drawable/empty"
        android:layout_below="@+id/textView4"
        android:layout_alignStart="@+id/location4"
        android:layout_marginTop="11dp" />

    <TextView
        android:text="3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView3"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/location3"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:text="4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView4"
        android:textColor="#ffffff"
        android:layout_alignBottom="@+id/location4"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:text="5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50sp"
        android:id="@+id/textView5"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/location5"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:text="Quantity:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/QuanitiyLabel"
        android:textColor="#000000"
        android:textSize="19sp"
        android:layout_alignBaseline="@+id/qtyinput"
        android:layout_alignBottom="@+id/qtyinput"
        android:layout_alignStart="@+id/liquorlabel" />

    <TextView
        android:text=""
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:textSize="17sp"
        android:textColor="#000000"
        android:id="@+id/liqinput"
        android:layout_below="@+id/location10"
        android:layout_toEndOf="@+id/location5" />

    <TextView
        android:text="Liquid: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="20sp"
        android:id="@+id/liquorlabel"
        android:layout_alignBaseline="@+id/liqinput"
        android:layout_alignBottom="@+id/liqinput"
        android:layout_toStartOf="@+id/liqinput" />

    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="#000000"
        android:id="@+id/qtyinput"
        android:layout_below="@+id/ezbarBox"
        android:layout_toEndOf="@+id/QuantityInput" />


</RelativeLayout>